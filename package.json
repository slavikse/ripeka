  "scripts": {
    "start": "npm run watch -- --env.android",
    "watch": "webpack --watch --env.tnsAction run",
    "build": "npm run build:android -- --env.android && npm run replacer",
    "build:android": "webpack --env.tnsAction build",
    "replacer": "./scripts/replacer.sh",
    "debug": "npm run debug:android -- --env.android",
    "debug:android": "webpack --watch --env.tnsAction debug",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "nativescript-audio": "^4.3.5",
    "nativescript-theme-core": "^1.0.4",
