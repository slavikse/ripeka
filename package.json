{
  "name": "ripeka",
  "version": "0.5.0",
  "description": "Мобильная игра крестики нолики.",
  "author": "slavikse <gslavikse@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/slavikse/ripeka.git"
  },
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "npm run watch:android",
    "watch:android": "npm run watch -- --env.android",
    "watch": "webpack --watch --env.mode=development --env.tnsAction run",
    "compile": "npm run build && npm run tns_build && npm run replace_apk",
    "tns_build": "cd dist && tns build android --release --key-store-path ../ripeka.jks --key-store-password 123456 --key-store-alias ripeka --key-store-alias-password 123456",
    "build:android": "npm run build -- --env.android",
    "build": "webpack --env.mode=production --env.tnsAction build",
    "replace_apk": "./scripts/replace_apk.sh",
    "debug:android": "npm run debug -- --env.android",
    "debug": "webpack --watch --env.mode=development --env.tnsAction debug",
    "clean": "rimraf dist",
    "count_line": "./scripts/count_line.sh"
  },
  "keywords": [
    "nativescript",
    "vue",
    "android",
    "tic tac toe",
    "крестики нолики"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "nativescript-admob": "^1.4.0",
    "nativescript-theme-core": "^1.0.4",
    "nativescript-vue": "^1.3.1",
    "tns-android": "^4.0.1",
    "tns-core-modules": "^4.0.0",
    "vue-axios": "^2.1.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs-extra": "^6.0.1",
    "nativescript-vue-externals": "^0.2.0",
    "nativescript-vue-loader": "^0.1.5",
    "nativescript-vue-target": "^0.1.0",
    "nativescript-vue-template-compiler": "^1.3.1",
    "node-sass": "^4.9.0",
    "ns-vue-loader": "^0.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.12.0",
    "webpack-synchronizable-shell-plugin": "0.0.7",
    "winston-color": "^1.0.0"
  }
}
